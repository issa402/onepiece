{
  "name": "react-typescript-mastery-module",
  "version": "1.0.0",
  "description": "Complete React + TypeScript mastery learning module with hands-on labs and real-world projects",
  "main": "01-react-typescript-fundamentals-lab.tsx",
  "scripts": {
    "start": "npx tsx 01-react-typescript-fundamentals-lab.tsx",
    "advanced": "npx tsx 02-advanced-patterns-lab.tsx",
    "todo-app": "npx tsx 03-todo-app-project.tsx",
    "dashboard": "npx tsx 04-ecommerce-dashboard-project.tsx",
    "chat-app": "npx tsx 05-realtime-chat-project.tsx",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "typescript",
    "frontend",
    "web-development",
    "hooks",
    "state-management",
    "components",
    "learning",
    "tutorial",
    "hands-on",
    "projects",
    "modern-javascript",
    "jsx",
    "tsx",
    "vite",
    "testing"
  ],
  "author": "One Piece Learning Modules",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "@reduxjs/toolkit": "^1.9.3",
